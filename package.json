    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.11",
    "vue-carousel": "^0.18.0",
    "vue-clickaway": "^2.2.2",
    "vue-lazyload": "^1.3.3",
    "vue-material": "^1.0.0-beta-15"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
